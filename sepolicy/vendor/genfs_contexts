# Fingerprint
genfscon sysfs /devices/platform/11012000.spi2/spi_master/spi2/spi2.0/authen_fd u:object_r:vendor_sysfs_fingerprint:s0
genfscon sysfs /devices/platform/11012000.spi2/spi_master/spi2/spi2.0/authen_fd/performance u:object_r:vendor_sysfs_fingerprint:s0
genfscon sysfs /devices/platform/11012000.spi2/spi_master/spi2/spi2.0/clk_enable u:object_r:vendor_sysfs_fingerprint:s0
genfscon sysfs /devices/platform/11012000.spi2/spi_master/spi2/spi2.0/irq u:object_r:vendor_sysfs_fingerprint:s0
genfscon sysfs /devices/platform/11012000.spi2/spi_master/spi2/spi2.0/modalias u:object_r:vendor_sysfs_fingerprint:s0
genfscon sysfs /devices/platform/11012000.spi2/spi_master/spi2/spi2.0/wakeup_enable u:object_r:vendor_sysfs_fingerprint:s0
genfscon sysfs /devices/platform/11012000.spi2/spi_master/spi2/statistics/modalias u:object_r:vendor_sysfs_fingerprint:s0
genfscon sysfs /devices/platform/11012000.spi2/spi_master/spi2/subsystem/modalias u:object_r:vendor_sysfs_fingerprint:s0

# Extcon
genfscon sysfs /devices/platform/extcon_usb/extcon/extcon0/cable.0/name u:object_r:sysfs_extcon:s0
genfscon sysfs /devices/platform/extcon_usb/extcon/extcon0/cable.1/name u:object_r:sysfs_extcon:s0
genfscon sysfs /devices/platform/fingerprint u:object_r:vendor_sysfs_fingerprint:s0

# Health
genfscon sysfs /devices/platform/mt_charger/power_supply/ac                                        u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/platform/battery/power_supply/battery                                      u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/platform/mt_charger/power_supply/usb                                       u:object_r:sysfs_batteryinfo:s0

# Touchpanel
genfscon sysfs /touchpanel                            						   u:object_r:sysfs_touchpanel:s0

# Wakeup (https://lkml.org/lkml/2019/8/6/1275)
genfscon sysfs /devices/platform/1000d000.pwrap/1000d000.pwrap:mt6358-pmic/mt6358-rtc/wakeup u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/1000d000.pwrap/1000d000.pwrap:mt6358-pmic/mt635x-auxadc/wakeup u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/10238000.gce/wakeup u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/11004000.i2c7/i2c-7/7-006a/wakeup u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/15020000.imgsys_config/wakeup u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/virtual/input/input2/wakeup u:object_r:sysfs_wakeup:s0
